{
  "name": "fetcher",
  "version": "0.1.3",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "env:save": "node ./scripts/senv-proxy.js --encrypt --target",
    "env:load": "node ./scripts/senv-proxy.js --decrypt --target",
    "env:toLive": "npm run env:load live",
    "env:toDev": "npm run env:load dev",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:live": "node dist/main",
    "stop": "npx pm2 stop fetcher",
    "status": "npx pm2 status",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "go": "npm run build && npm run start:live",
    "docker:build": "docker build --platform linux/amd64 --tag scrape .",
    "docker:build:live": "docker build --progress=plain --platform linux/amd64 --tag 301891243765.dkr.ecr.us-east-2.amazonaws.com/v47-fetch .",
    "docker:aws:login:tdog": "docker login -u AWS -p $(aws ecr get-login-password --region us-east-2 --profile v47-tdog) 301891243765.dkr.ecr.us-east-2.amazonaws.com",
    "docker:run": "docker run --name scrape --cap-add=SYS_ADMIN --publish 6112:6112 scrape",
    "docker:push": "docker push 301891243765.dkr.ecr.us-east-2.amazonaws.com/v47-fetch",
    "docker:stop": "docker stop fetcher",
    "docker:rm": "docker rm fetcher",
    "deploy": "npm run env:toLive && npm run docker:build:live && npm run docker:aws:login:tdog && npm run docker:push && npm run env:toDev"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.682.0",
    "@aws-sdk/client-sqs": "^3.682.0",
    "@aws-sdk/lib-storage": "^3.682.0",
    "@aws-sdk/s3-request-presigner": "^3.682.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.7",
    "@nestjs/config": "^3.2.1",
    "@nestjs/core": "^10.3.7",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/swagger": "^7.3.1",
    "@nexrender/api": "^1.50.1",
    "@supabase/supabase-js": "^2.41.1",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "fluent-ffmpeg": "^2.1.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "node-localstorage": "^3.0.5",
    "openai": "^4.32.0",
    "puppeteer": "^22.6.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "senv": "^1.0.2",
    "sharp": "^0.33.4",
    "slugify": "^1.6.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.7",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
